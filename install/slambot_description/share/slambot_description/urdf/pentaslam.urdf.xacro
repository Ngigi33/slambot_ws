<?xml version="1.0"?>
<robot name="pentaslam"
    xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find pentaslam_description)/urdf/pentaslam_gazebo.xacro"/>

    <xacro:property name="PI" value="3.14159"/>
    <xacro:property name="effort" value="30"/>
    <xacro:property name="velocity" value="10.0"/>

    <xacro:macro name="default_inertial" params="mass">
        <inertial>
            <mass value="${mass}"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </xacro:macro>

    <link name="base_link"/>

    <joint name="joint_name" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0.0 0.0 0.07" rpy="0.0 0.0 3.142"/>
    </joint>


    <link name="chassis">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/ROBOT_ASSEMBLY.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/ROBOT_ASSEMBLY.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- CASTOR WHEEL1 LINK -->

    <joint name="caster_wheel_joint1" type="fixed">
        <parent link="chassis"/>
        <child link="castor_link1"/>
    <origin xyz="-0.06580 0.0657 -0.062" rpy="0.0 0.0 3.142"/>
    </joint>

    <link name="castor_link1">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/Eye_Ball.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/Eye_Ball.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- CASTOR WHEEL2 LINK -->

    <joint name="caster_wheel_joint2" type="fixed">
        <parent link="chassis"/>
        <child link="castor_link2"/>
        <origin xyz="-0.06640 -0.0425 -0.062" rpy="0.0 0.0 3.142"/>
    </joint>

    <link name="castor_link2">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/Eye_Ball.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/Eye_Ball.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- LEFT WHEEL LINK -->
    <joint name="left_wheel_joint" type="continuous">
        <parent link="chassis"/>
        <child link="left_wheel"/>
        <origin xyz="0.074757 -0.120900 -0.024" rpy="0.0 0.0 3.142"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="left_wheel">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/85mm_Wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/85mm_Wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- RIGHT WHEEL LINK -->
    <joint name="right_wheel_joint" type="continuous">
        <parent link="chassis"/>
        <child link="right_wheel"/>
        <origin xyz="0.074757 0.126900 -0.024" rpy="0.0 0.0 0.0"/>
        <axis xyz="0 -1 0 "/>
    </joint>

    <link name="right_wheel">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/85mm_Wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/85mm_Wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- LIDAR LINK-->
    <joint name="lidar_joint" type="fixed">
        <parent link="chassis"/>
        <child link="lidar"/>
        <origin xyz="0.08100009757 0.005 0.0988" rpy="0.0 0.0 1.57"/>
    </joint>

    <link name="lidar">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/LidarTop.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/LidarTop.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- IMU_LINK -->
    <joint name="imu_joint" type="fixed">
        <parent link="chassis"/>
        <child link="imu"/>
        <origin xyz="0.0002 0.0084 0.0766" rpy="1.57 0.0 -1.57"/>
    </joint>

    <link name="imu">
        <xacro:default_inertial mass="1.0"/>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/MPU60501.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://pentaslam_description/meshes/MPU60501.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

</robot>